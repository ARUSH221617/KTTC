{
  "name": "KTTC",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development next dev -p 3000 2>&1 | tee dev.log",
    "build": "prisma generate --schema=db/schema.prisma && cross-env NODE_ENV=production next build && cp -r .next/static .next/standalone/.next/ && cp -r public .next/standalone/",
    "start": "cross-env NODE_ENV=production node .next/standalone/server.js 2>&1 | tee server.log",
    "lint": "next lint",
    "db:push": "prisma db push --schema=db/schema.prisma",
    "db:generate": "prisma generate --schema=db/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=db/schema.prisma",
    "db:migrate:prod": "prisma migrate deploy --schema=db/schema.prisma",
    "db:reset": "prisma migrate reset --schema=db/schema.prisma",
    "db:seed": "prisma db seed",
    "test": "jest",
    "db:studio": "prisma studio --schema=db/schema.prisma"
  },
